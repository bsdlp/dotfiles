Host github.com
  StrictHostKeyChecking No
  ControlMaster No

ControlMaster auto
ControlPath ~/.ssh/sock/%r@%h:%p;%n
ControlPersist 30M

Host control
  StrictHostKeyChecking No
  Hostname nerdrage.biz
  User root

Host irssi
  StrictHostKeyChecking No
  Hostname 23.92.19.129
  Port 2222
  User jchen

Host ewr0
  StrictHostKeyChecking No
