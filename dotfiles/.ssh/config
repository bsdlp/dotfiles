  User szuhchen

host git-aws.internal.justin.tv git.xarth.tv
    ProxyJump szuhchen@bastion.git.xarth.tv

Host github.com
  ControlMaster no

ControlMaster auto
ControlPath ~/.ssh/sock/%r@%h:%p;%n
ControlPersist 30M

host *
  User jchen
