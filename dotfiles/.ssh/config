StrictHostKeyChecking no
UserKnownHostsFile /dev/null

Host git.bsdlp.net
  Hostname 192.168.201.7

Host raspberrypi
  HostName raspberrypi
  ForwardAgent yes
  User pi

Host github.com
  User jchen
  ControlMaster no

ControlMaster auto
ControlPath ~/.ssh/sock/%r@%h:%p;%n
ControlPersist 30M

Host *
  User jchen

