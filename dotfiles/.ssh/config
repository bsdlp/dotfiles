StrictHostKeyChecking no
UserKnownHostsFile /dev/null

Host raspberrypi
  HostName raspberrypi
  ForwardAgent yes
  User pi

Host github.com
  User jchen
  ControlMaster no

ControlMaster auto
ControlPath ~/.ssh/sock/%r@%h:%p;%n
ControlPersist 30M

Host *
  User jchen

Host dev
  HostName 107.150.36.10
  ForwardAgent yes
  User jchen
