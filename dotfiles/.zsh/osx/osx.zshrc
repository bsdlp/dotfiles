# set _ENVIRONMENT envvar
export _ENVIRONMENT="osx"

# gnu coreutil aliases
alias ls='gls --color=auto --group-directories-first'
alias l='ls'
alias mv='gmv -i'
alias cp='gcp -i'
alias rm='grm -i'
alias sed='gsed'

# Go jawns
export GOROOT=/usr/local/opt/go/libexec

# Apple logo!
~/.zsh/archey-osx -c

